{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
        },
    "extends": "standard-with-typescript",
    "plugins" : ["prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unsed-vars": "warn",
        "no-console": "warn",
        "prettier/prettier" : "warn",
        "func-names": "off"
    }
}
